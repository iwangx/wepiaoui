                                        <tr>
                                            <td>列1</td>
                                            <td>列2</td>
                                            <td>列3</td>
                                            <td>列4</td>
                                            <td>列5</td>
                                            <td>列6</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </pre>
                        </div>
                    </div>

                    <p>带操作按钮的表格</p>
                    <div class="demo">
                        <div class="demo-content">
                            <table class="tb">
                                <thead class="tb-head">
                                <tr>
                                    <td>列1</td>
                                    <td>列2</td>
                                    <td>列3</td>
                                    <td>列4</td>
                                    <td>列5</td>
                                    <td>列6</td>
                                </tr>
                                </thead>
                                <tbody class="tb-body">
                                <tr>
                                    <td>列1</td>
                                    <td>列2</td>
                                    <td>列3</td>
                                    <td>列4</td>
                                    <td>列5</td>
